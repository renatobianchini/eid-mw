#: src/dialogsgtk/badpindialog.c:55
msgid "beID: Incorrect PIN Code"
msgstr "beID: Foutive PINcode"

#: src/dialogsgtk/changepindialog.c:152
msgid "beID: Change PIN Code"
msgstr "beID: PIN Code Wijzigen"

#: src/dialogsgtk/changepindialog.c:161
