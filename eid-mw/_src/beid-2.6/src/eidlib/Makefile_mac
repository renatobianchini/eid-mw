dist: 
	@mkdir -p .tmp/beid && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/beid/ && ( cd `dirname .tmp/beid` && $(TAR) beid.tar beid && $(GZIP) beid.tar ) && $(MOVE) `dirname .tmp/beid`/beid.tar.gz . && $(DEL_FILE) -r .tmp/beid

